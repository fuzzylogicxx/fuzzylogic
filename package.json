{
  "name": "eleventy-base-blog",
  "version": "5.0.0",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "watch:sass": "sass --watch --style=compressed src/sass:_includes/css",
    "build:sass": "sass --style=compressed src/sass:_includes/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "build:lambda": "netlify-lambda build _lambda",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "ELEVENTY_ENV=prod npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "eslint": "^7.23.0"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-cache-assets": "^2.0.3",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
    "dotenv": "^8.2.0",
    "encoding": "^0.1.12",
    "luxon": "^1.26.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.1.0",
    "netlify-lambda": "^1.6.3",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "purgecss": "^4.0.3",
    "sass": "^1.32.8",
    "slugify": "^1.4.7",
    "terser": "^5.6.1"
  }
}
