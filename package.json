{
  "name": "eleventy-base-blog",
  "version": "5.0.0",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "serve": "gulp build & gulp watch & ELEVENTY_ENV=dev npx @11ty/eleventy --serve",
    "serve:prod": "gulp build & gulp watch & ELEVENTY_ENV=prod npx @11ty/eleventy --serve",
    "watch": "npx eleventy --watch",
    "debug": "DEBUG=* npx eleventy",
    "wipe": "gulp wipe",
    "build": "gulp wipe && gulp build & ELEVENTY_ENV=prod eleventy && npm run build:lambda",
    "build:lambda": "netlify-lambda build _lambda"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "luxon": "^1.12.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "purgecss": "^2.3.0"
  },
  "dependencies": {
    "@11ty/eleventy-cache-assets": "^2.0.3",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "dotenv": "^8.2.0",
    "eleventy-plugin-respimg": "^0.1.2",
    "encoding": "^0.1.12",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "netlify-lambda": "^1.6.3",
    "node-fetch": "^2.6.1",
    "slugify": "^1.3.6"
  }
}
